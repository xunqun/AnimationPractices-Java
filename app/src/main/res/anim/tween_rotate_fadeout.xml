<?xml version="1.0" encoding="utf-8"?><set xmlns:android="http://schemas.android.com/apk/res/android"    android:shareInterpolator="false">    <set        android:interpolator="@android:anim/accelerate_interpolator"        android:startOffset="700">        <scale            android:duration="600"            android:fromXScale="1"            android:fromYScale="1"            android:pivotX="50%"            android:pivotY="50%"            android:toXScale="1.5"            android:toYScale="1.5" />        <rotate            android:duration="600"            android:fromDegrees="0"            android:pivotX="50%"            android:pivotY="50%"            android:toDegrees="360"            android:toYScale="0.0" />    </set></set>